{
  "npmScope": "myorg",
  "affected": {
    "defaultBase": "master"
  },
  "implicitDependencies": {
    "angular.json": "*",
    "package.json": {
      "dependencies": "*",
      "devDependencies": "*"
    },
    "tsconfig.base.json": "*",
    "tslint.json": "*",
    ".eslintrc.json": "*",
    "nx.json": "*"
  },
  "tasksRunnerOptions": {
    "default": {
      "runner": "@nrwl/workspace/tasks-runners/default",
      "options": {
        "cacheableOperations": ["build", "lint", "test", "e2e"]
      }
    }
  },
  "projects": {
    "onlinestore-admin": {
      "tags": []
    },
    "onlinestore-admin-e2e": {
      "tags": [],
      "implicitDependencies": ["onlinestore-admin"]
    },
    "infrastructure-logger": {
      "tags": []
    },
    "pricing-coupons": {
      "tags": ["scope:pricing", "type:feature"]
    },
    "sales-product-editor": {
      "tags": ["scope:sales", "type:feature"]
    },
    "sales-products": {
      "tags": ["scope:sales", "type:feature"]
    },
    "customers-user-editor": {
      "tags": ["scope:customers", "type:feature"]
    },
    "customers-users": {
      "tags": ["scope:customers", "type:feature"]
    },
    "shared-ng-material": {
      "tags": ["scope:shared", "type:ui"]
    },
    "shared-layout": {
      "tags": ["scope:shared", "type:ui"]
    },
    "onlinestore-client": {
      "tags": ["type:app", "scope:client"]
    },
    "onlinestore-client-e2e": {
      "tags": [],
      "implicitDependencies": ["onlinestore-client"]
    }
  }
}
